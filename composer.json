{
    "name": "thomasderooij/laravel-modules",
    "type": "project",
    "description": "Laravel framework modules kit.",
    "keywords": [
        "framework",
        "laravel",
        "modules",
        "modular"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "laravel/framework": "^7.30",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^5.14",
        "spatie/phpunit-snapshot-assertions": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Thomasderooij\\LaravelModules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomasderooij\\LaravelModules\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomasderooij\\LaravelModules\\ModuleServiceProvider",
                "Thomasderooij\\LaravelModules\\ConsoleSupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
