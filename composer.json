{
    "name": "thomasderooij/laravel-modules",
    "description": "Laravel framework modules kit.",
    "keywords": [
        "framework",
        "laravel",
        "thomasderooij",
        "modules",
        "module"
    ],
    "authors": [
        {
            "name": "Thomas de Rooij",
            "email": "thomasderooij@gmail.com",
            "role": "developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|8.0",
        "laravel/framework": "^8",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.12",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Thomasderooij\\LaravelModules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomasderooij\\LaravelModules\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomasderooij\\LaravelModules\\ModuleServiceProvider",
                "Thomasderooij\\LaravelModules\\ConsoleSupportServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
